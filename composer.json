{
    "name": "eightbitsnl/policy-and-permission-generator",
    "description": "Policy and Permission Generator",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Eightbitsnl\\PolicyAndPermissionGenerator\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Loek Wetzels",
            "email": "loek.wetzels@gmail.com",
            "homepage": "https://eightbits.nl",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.2.0",
        "illuminate/auth": "^7.0|^8.0|^9.0",
        "illuminate/cache": "^7.0|^8.0|^9.0",
        "illuminate/support": "^7.0|^8.0|^9.0",
        "spatie/laravel-permission": "^3.0|^4.0|^5.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Eightbitsnl\\PolicyAndPermissionGenerator\\PolicyAndPermissionGeneratorServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}